{
  "name": "react-dd-menu",
  "version": "1.0.6",
  "description": "A react dropdown menu.",
  "main": "dist/index.js",
  "scripts": {
    "scripts": "babel ./src/js --out-dir ./dist",
    "styles": "sass --style expanded --line ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.css && sass --style compressed ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.min.css",
    "prebuild": "rimraf dist",
    "build": "npm run scripts && npm run styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-dd-menu.git"
  },
  "keywords": [
    "react",
    "react-component",
    "dropown",
    "dropdown-menu",
    "component"
  ],
  "author": "mlaursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-dd-menu/issues"
  },
  "homepage": "https://github.com/mlaursen/react-dd-menu#readme",
  "dependencies": {
    "classnames": "^2.1.3"
  },
  "peerDependencies": {
    "react": "^0.14.x",
    "react-dom": "^0.14.x",
    "react-addons-css-transition-group": "^0.14.x",
    "react-addons-pure-render-mixin": "^0.14.x"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12"
  }
}
